name: '<{{ inventory_hostname }}>'
listen-peer-urls: 'http://{{ hostvars[inventory_hostname]['ansible_eth2']['ipv4']['address'] }}:2380'
listen-client-urls: 'http://{{ hostvars[inventory_hostname]['ansible_eth2']['ipv4']['address'] }}:2379,http://127.0.0.1:2379'
initial-advertise-peer-urls: 'http://{{ hostvars[inventory_hostname]['ansible_eth2']['ipv4']['address'] }}:2380'
advertise-client-urls: 'http://{{ hostvars[inventory_hostname]['ansible_eth2']['ipv4']['address'] }}:2379'

initial-cluster: '<co7-node01>=http://{{ hostvars['co7-node01']['ansible_eth2']['ipv4']['address'] }}:2380,<co7-node02>=http://{{ hostvars['co7-node02']['ansible_eth2']['ipv4']['address'] }}:2380,<co7-node03>=http://{{ hostvars['co7-node03']['ansible_eth2']['ipv4']['address'] }}:2380'
initial-cluster-state: 'new'
initial-cluster-token: 'etcd-cluster-1'
data-dir: '/var/lib/pgsql/data'
